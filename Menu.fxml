<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.MenuController">
   <children>
      <ImageView fitHeight="450.0" fitWidth="542.0" layoutY="2.0" pickOnBounds="true" preserveRatio="true">
      	<image>
      		<Image url="/img/espace.jpg" />
      	</image>
      </ImageView> 
      <Button layoutX="128.0" layoutY="179.0" mnemonicParsing="false" onAction="#play" prefHeight="39.0" prefWidth="113.0" text="Play">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button layoutX="128.0" layoutY="257.0" mnemonicParsing="false" text="Parameters">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="exit" layoutX="128.0" layoutY="334.0" mnemonicParsing="false" onAction="#quit" prefHeight="39.0" prefWidth="113.0" text="Exit">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Label contentDisplay="TOP" layoutX="48.0" layoutY="48.0" prefHeight="73.0" prefWidth="318.0" text="In the Space" textFill="PINK">
         <font>
            <Font size="50.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
